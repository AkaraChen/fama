# EditorConfig - generated by fama (go-fmt style)
# https://editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = tab
indent_size = 4
tab_width = 4
max_line_length = 80

# JavaScript/TypeScript
[*.{js,jsx,ts,tsx,mjs,cjs}]
quote_type = double

# Shell scripts
[*.{sh,bash,zsh}]
indent_style = tab
indent_size = 4
